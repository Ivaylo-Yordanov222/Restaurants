@model MostSoldTableBindingModel
<form class="searching-form" id="most_sold_table" asp-area="Admin" asp-controller="Home" asp-action="MostSoldTables" method="post">
    <div class="form-row">
        <div class="form-group col-md-4">
            <label for="inlineFormInputGroup">от Дата</label>
            <input class="form-control" type="date" value="@DateTime.Now" asp-for="StartTime">
            <span asp-validation-for="StartTime" class="text-danger"></span>
        </div>
        <div class="form-group col-md-4">
            <label>до Дата</label>
            <input class="form-control" type="date" value="@DateTime.Now" asp-for="EndTime">
            <span asp-validation-for="EndTime" class="text-danger"></span>
        </div>

        <div class="form-group col-md-2">
            <input class="btn btn-dark form-control" style="margin-top:31px;" type="submit" value="Търси" />
        </div>
    </div>
</form>
